{
  "name": "back-erp",
  "version": "1.0.0",
  "description": "ERP server side",
  "author": "Renan",
  "license": "ISC",
  "engines": {
    "node": "20.9.0"
  },
  "scripts": {
    "start": "tsx src/server.ts",
    "start:dev": "tsx watch src/server.ts",
    "build": "tsup src",
    "test": "vitest -c ./vitest.config.unit.ts",
    "test:ui": "vitest -c ./vitest.config.unit.ts --ui",
    "test:int": "./scripts/run-integration.sh",
    "test:int:ui": "./scripts/run-integration.sh --ui",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "generate": "npx prisma generate",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "studio": "npx prisma studio",
    "postinstall": "npm run migrate:dev",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.50.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prisma": "^5.4.1",
    "prisma-erd-generator": "^1.11.1",
    "supertest": "^6.3.3",
    "tsup": "^7.2.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5",
    "vitest-mock-extended": "^1.3.1"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
